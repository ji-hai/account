    "@types/qs": "^6.9.3",
    "@vue/babel-preset-app": "^4.4.6",
    "@vue/cli-plugin-babel": "^4.4.6",
    "@vue/cli-plugin-typescript": "^4.4.6",
    "@vue/cli-service": "^4.4.6",
    "babel-loader": "^8.1.0",
    "babel-plugin-import": "^1.13.3",
    "core-js": "^3.6.5",
    "node-sass": "^4.14.1",
